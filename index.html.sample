<!DOCTYPE html>
<meta charset="utf-8">

<head>
  <link rel="stylesheet" href="css/styles.css">
  <title>LoRa GPS Tracker</title>
</head>

<!-- <body onload="initMap()"> -->
<body>
  <header class="bg-primary text-white text-center">
    <div>
      <span id="status">Connection Status: Not Connected</span>
      <span id="status_messages"></span>
    </div>
  </header>
  <!--<button type="button" onClick='onPause()'>Pause</button>
  <button type="button" onClick='onContinue()'>Continue</button>-->
  <br>
  <table id="loradata">
    <tr>
      <th class="datos">
        <table class="tg">
          <tr>
            <td class="tg-yw4l">Device Name</td>
            <td class="tg-yw4l">Latitude</td>
            <td class="tg-yw4l">Longitude</td>
            <td class="tg-yw4l">Battery Level (%)</td>
            <td class="tg-yw4l">Time Stamp</td>
          </tr>
        </table>
      </th>
    </tr>
  </table>
  <br>
  <div id="map"></div>
  <script type="text/javascript" src="js/jquery.1.8.2.min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=YOUR_API_KEY">
</script>
  <script src="js/moment.min.js"></script>
  <script src="js/d3.v4.min.js"></script>
  <script src="js/mqttws31.js" type="text/javascript"></script>
  <script type="text/javascript" src="js/main.js"></script>
</body>

</html>
